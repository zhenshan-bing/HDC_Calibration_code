	    <color rgba="0.5 0.5 0.5 0.8"/>
	  </material>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 1.25 0"/>
      <geometry>
	 	<box size="2 0.5 0.5"/>
      </geometry>
    </collision>
  </link>

